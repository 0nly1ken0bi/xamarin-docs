{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml",
          "**/*.html"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/includes/**"
        ]
      }
    ],
    "markdownEngineName": "markdig",
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId":"MSDocsHeader-Xamarin",
      "breadcrumb_path": "/xamarin/breadcrumb/toc.json",
      "titleSuffix": "Xamarin",
      "extendBreadcrumb": true,
      "manager": "crdun",
      "audience": "developer",
      "ms.topic": "article",
      "ms.technology": "xamarin",
      "ms.prod": "xamarin",
      "searchScope": ["Xamarin"],

      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/xamarin-docs",
      "feedback_product_url": "https://forums.xamarin.com/categories/general/"
    },
    "fileMetadata": {
      "feedback_product_url": {
        "android/**/**.md":"https://forums.xamarin.com/categories/android/",
        "ios/**/**.md":"https://forums.xamarin.com/categories/ios/",
        "mac/**/**.md":"https://forums.xamarin.com/categories/mac/",
        "xamarin-forms/**/**.md":"https://forums.xamarin.com/categories/xamarin-forms/",
        "cross-platform/**/**.md":"https://forums.xamarin.com/categories/cross-platform/",
        "tools/workbooks/**/**.md":"https://forums.xamarin.com/categories/inspector/",
        "tools/inspector/**/**.md":"https://forums.xamarin.com/categories/inspector/",
        "tools/live-player/**/**.md":"https://forums.xamarin.com/categories/live-player",
        "tools/profiler/**/**.md":"https://forums.xamarin.com/categories/profiler",
        "graphics-games/**/**.md":"https://forums.xamarin.com/categories/graphics-games/"
      }
    },
    "template": [],
    "dest": "xamarin-docs"
  }
